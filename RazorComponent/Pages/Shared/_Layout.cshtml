    <script src="~/scripts/BaseConfiguration.js" asp-append-version="true"></script>
    <script src="~/Assets/plugin/bootstrap-datepicker/bootstrap-datepicker-vue.js"></script>
    <script src="~/Index?handler=Config&v=" asp-append-version="true"></script>

    @await RenderSectionAsync("app", required: false)
</body>
</html>
