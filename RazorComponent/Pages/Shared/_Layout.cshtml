    <link href="~/Assets/plugin/Datatable/DataTableConfig.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>


    <script src="~/Assets/plugin/Vue/vue.min.js"></script>
    <script src="~/Assets/plugin/Vue/vue-class-component.min.js"></script>
    <script src="~/Assets/plugin/Vue/VuePropertydecorator.umd.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
